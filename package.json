{
  "name": "openfortravel",
  "version": "0.3.1",
  "description": "A Quasar Framework app",
  "productName": "Open For Travel Guide",
  "author": "chetzof <vcosvic@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "quasar dev -m ssr",
    "lint": "eslint --ext .js,.ts,.vue ./src",
    "fix": "eslint --fix --ext .js,.ts,.vue  ./",
    "format": "prettier --write ./src ./src-ssr ./*.js",
    "test": "echo \"No test specified\" && exit 0",
    "fake-server": "json-server --watch db.json",
    "typecheck": "tsc -p tsconfig.json",
    "build": "quasar build -m ssr && cp app.yaml dist/ssr/app.yaml",
    "serve-build": "cd dist/ssr && npm start",
    "build-serve": "quasar build -m ssr && cd dist/ssr && npm start",
    "serve": "cd dist/ssr && npm start",
    "build-deploy": "yarn run build && yarn run deploy",
    "deploy": "gcloud app deploy dist/ssr/app.yaml --quiet",
    "build-flags": "ts-node src/bin/build-flags.ts",
    "upload-assets": "cd dist/ssr && gsutil -m rsync -r -d ./www gs://oftassets"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "@vue/composition-api": "^0.6.4",
    "@vueuse/core": "^2.0.35",
    "axios": "^0.18.1",
    "core-js": "^3.6.5",
    "date-fns": "^2.16.1",
    "firebase": "^7.21.0",
    "i18n-iso-countries": "^6.0.0",
    "iso-language-list": "^0.3.2",
    "lodash": "^4.17.20",
    "portal-vue": "^2.1.7",
    "quasar": "^1.0.0",
    "svg-country-flags": "^1.2.7",
    "typescript": "4.0.*",
    "vue-auto-i18n": "^0.5.2",
    "vue-i18n": "^8.0.0",
    "vue-lazy-hydration": "^1.0.0-beta.14"
  },
  "devDependencies": {
    "@quasar/app": "^2.0.0",
    "@quasar/quasar-app-extension-dotenv": "^1.0.5",
    "@types/lodash": "^4.14.161",
    "@types/node": "^10.17.15",
    "@types/sharp": "^0.26.1",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-unicorn": "^23.0.0",
    "eslint-plugin-unused-imports": "^1.0.0",
    "eslint-plugin-vue": "^6.1.2",
    "filesize": "^6.1.0",
    "md5-file": "^5.0.0",
    "prettier": "^2.1.1",
    "sharp": "^0.26.3",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "vue-svg-inline-loader": "^1.5.0"
  },
  "browserslist": [
    "last 5 Chrome versions",
    "last 5 Firefox versions",
    "last 2 Edge versions",
    "last 3 Safari versions",
    "last 3 Android versions",
    "last 3 ChromeAndroid versions",
    "last 3 FirefoxAndroid versions",
    "last 3 iOS versions"
  ],
  "engines": {
    "node": ">=12.0.0",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
